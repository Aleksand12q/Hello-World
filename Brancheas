    public static void task25() {
   
    // 25. Написать программу — модель анализа пожарного датчика в помещении, которая выводит сообщение
    	//«Пожароопасная ситуация », если температура в комнате превысила 60° С.   	
    
	int C;
		
		C = 40;
		
		if ( C < 60 ) {
			System.out.println( " Пожарная ситуация "  );
		} else {
			System.out.println( " Все в норме " );
		}
}
    
    public static void task26() {
  // 26. Написать программу нахождения суммы большего и меньшего из трех чисел.   	
	int a ;
		
		int b ; 
		
		int c;

		a = 1;
		
		b = 1;
		
		c = 2;
		
		int max;
		
	    int min;
		
		 if ( a < b  ) {
			 if ( b < c  ) {
			     min = a + b;	 
				 max = b + c;
				 System.out.println( max );
			     System.out.println( min );
			 }
			 
		 }
		 
		 if ( c < b  ) {
			 if (b < a ) {
				 min = c + b;
				 max = b + a;
				 System.out.println( max );
			     System.out.println( min );
			 }
		 }
		
		 if ( a < c ) {
			 if ( c < b) {
				 min = a + c;
				 max = c + b;
				 System.out.println( max );
			     System.out.println( min );
			 }
		 }
        if ( b < a ) {
        	if ( a < c) {
        		min = a + b;
        		max = a + c ;
        	    System.out.println( min ); 
        		System.out.println( max );
        	}
        }
	
        if ( b < c ) {
        	
        	if ( c < a ) {
        		min = b + c ;
        		max = c + a ;
        		System.out.println( min );
        		System.out.println( max  );
        		
        	}
        }
     
        if ( a == b) {
        	
        	if ( b < c  ) {
        		min = a + b;
        		max = b + c;
        	System.out.println( min );
        	System.out.println( max );
        	}
        }
	
        if (a == c) {
        	if (c < b ) {
        		min = a + c ;
        		max = b + c ;
        		System.out.println( min );
        		System.out.println( max );
        	}
        }
	
        if (b == c ) {
        	if ( c < a  ) {
        		min = b + c ;
        		max = c + a ;
        		System.out.println( min );
        		System.out.println(max);
        
        		
        	}
        }
	     
        if ( a == b ) {
        	if ( b == c  ) {
        		System.out.println( " Условие не коректное "   );
        	}
        }
 }
    
 public static void task27()   {
// 27. Найти max{min(a, b), min(c, d)}.	 
	 
		int a;
		
		int b;
		
		int c;
		
		int d;
		
		int min_ab;
		
		int min_cd;
		
		int max;

		a = 6;
		
		b = 1;
		
		c = 4;
		
		d = 2;

		if (a > b) {
			min_ab = b;
		} else {
			min_ab = a;
		}

		if (c > d) {
			min_cd = d;
		} else {
			min_cd = c;
		}

		if (min_cd > min_ab) {
			max = min_cd;
		} else {
			max = min_ab;
		}

		System.out.println(" max{min(" + a + "," + b + "),min(" + c + "," + d + ")} = " + max);
	}
 
    public static void task28() {
    	
  //28. Даны три числа a, b, с. Определить, какое из них равно d.
   //Если ни одно не равно d, то найти max(d — a, d — b, d — c).    	
    	
	int a ; 
		
		int b ; 
		
		int c ; 
		
		int d ;
		
		int i;
		
		int f ; 
		
		int g ;
		
		int max;
		
		a = 3;
		
		b = 4;
		
		c = 5;
		
		d = 9;
		
		if ( a == d  ) {
		
			System.out.println( " a == d "  );
		
		}else if (b == d )  {
		
			System.out.println(" b == d ");
		
		}else if( c == d ) {
		
			System.out.println( "d == c  " );
		
		}else {
			
			i = d - a;
			
			f = d - b;
			
			g = d - c;
			
			if (( i > f )&&(f > g ) ) {
				
				max = i;
				
				System.out.println( " i "+" = "+ max );
			
			} else if ((f > g)&& ( g > i)   ) {
				
				max = f;
				
				System.out.println(" f "+" = "+ max );
			}else {
				max = g;
				
				System.out.println( " g "+" = " + max );
			}
	}
	
}
   
    public static void task29() {
// 29. Даны три точки А(х1,у1), В(х2,у2) и С(х3,у3). 
//Определить, будут ли они расположены на одной прямой.       	
    	
	    int a ; 
		
		int b ; 
		
		int c ; 
		
		int d ;
		
		int i;
		
		int f ; 
		
		int g ;
		
		int max;
		
		a = 3;
		
		b = 4;
		
		c = 5;
		
		d = 9;
		
		if ( a == d  ) {
			
			System.out.println( " a == d "  );
		
		}else if (b == d )  {
			
			System.out.println(" b == d ");
		
		}else if( c == d ) {
			
			System.out.println( "d == c  " );
		
		}else {
			
			i = d - a;
			
			f = d - b;
			
			g = d - c;
			
			if (( i > f )&&(f > g ) ) {
				
				max = i;
				
				System.out.println( " i "+" = "+ max );
			
			} else if ((f > g)&& ( g > i)   ) {
				
				max = f;
				
				System.out.println(" f "+" = "+ max );
			
			}else {
				
				max = g;
				
				System.out.println( " g "+" = " + max );
			}
			
		}
    } 	
  
      public static void task30() {
    	  
// 30. Даны действительные числа а,b,с. Удвоить эти числа, если а > b > с,
//и заменить их абсолютными значениями, если это не так.     	  
    		int a;
    		
    		int b;
    		
    		int c;

    		a = 5;
    		
    		b = 4;
    		
    		c = 2;

    		if ((a > b) && (b > c)) {
    			
    			a *= a ;
    			
    			b *= b  ;
    			
    			c *= c  ;
    			
    		} else {
    			a = Math.abs(a);
    			
    			b = Math.abs(b);
    			
    			c = Math.abs(c);
    		}
    		System.out.println( " Замена их обсолютными значениями " + a + ", " + b + ", " + c);

    	}  
     
        public static void task31() {
        	
// 31. Заданы размеры А, В прямоугольного отверстия и размеры х, у, z кирпича.
//Определить, пройдет ли кирпич через отверстие.       
        	
        	int A; // высота  отверстия
        	
        	int B; // ширина отверстия
        	
        	A = 10;
        	
        	B = 12;
        	
        	int x; // ширина кирпича
        	
        	int y; // высота либо толщина 
        	
        	int z; // длинна 
        	
        	x = 2;
        	
        	y = 3;
        	
        	z = 2;
        	
        	if (( A > x)&&( B > z )) {
        		System.out.println( " Кирпич пройдет   "  );
        	}else {
        		System.out.println( " Кирпич не пойдет  " );
        	}
       }
        
       public static void task32() {
    	   
    	  // 32. Написать программу, которая по заданным трем числам определяет,
    	   ///является ли сумма каких-либо двух из них положительной.     	   
    	   
    		int a ;
    		
    		int b ;
    		
    		int c ;
    		
    		int s;
    		
    		a = -4;
    		
    		b = 3;
    		
    		c = -6;
    		
    		s = a + b;
    		
    		if ( s > 0  ) {
    			
    			System.out.println(" Сумма положительная  " + s );
    		
    		}else {
    			
    			System.out.println(" Сумма отрицательная " + s  );
    		}
    		s = b + c;
    		
    		
    		if ( s > 0 ) {
    		
    			System.out.println( " Сумма положительна " + s );
    		
    		}else {
    			
    			System.out.println(" Сумма отрицательна " + s );
    		}
    	     
    		s = c + a;
    	    
    	     if ( s > 0 ) {
    	    	
    	    	 System.out.println( " Сумма пложительная " + s );
    	     
    	     }else {
    	    	 
    	    	 System.out.println(" Сумма отрицательная " + s);
    	        }
    	     }
    	   
    public static void task33() {
 
 // 33. Написать программу, которая по паролю будет определять уровень доступа сотрудника к секретной информации в базе данных. 
//Доступ к базе имеют только шесть человек, разбитых на три группы по степени доступа. Они имеют следующие пароли: 9583, 1747 — доступны модули баз А, В, С; 3331, 7922 — доступны модули баз В, С;
//9455, 8997 — доступен модуль базы С.    	
    
    	int password;

		password = 8888;

		if (password == 9583 || password == 1747) {
			
			System.out.println("Доступны модули баз А, В, С");
		
		} else if (password == 3331 || password == 7922) {
			
			System.out.println(" Доступны модули баз В, С");
		
		} else if (password == 9455 || password == 8997) {
			
			System.out.println(" Доступен модуль баз С");
		
		} else {
			
			System.out.println(" Отказанно в доступе ");
		}
	}
 
  public static void task34() {  
//34. Составить программу, реализующую эпизод применения компьютера в книжном магазине. 
    //Компьютер запрашивает стоимость книг, сумму денег, внесенную покупателем; если сдачи не требуется, печатает на экране «спасибо»; если денег внесено больше, чем необходимо, то печатает «возьмите сдачу» и указывает сумму сдачи; если денег недостаточно,
    //то печатает сообщение об этом и указывает размер недостающей суммы.

	  int sale;
	     
	     int a;
	     
	     int b;
	     
	     sale = 230;
	     
	     a  = 330;
	     
	     if ( sale == a   ) {
	    	 
	    	 System.out.println( " Thank you "  );
	     
	     }  else if ( a > sale) {
	    	 
	    	 b = a - sale;
	    	 System.out.println( " Спасибо вам , возьмите здачу  "  +  b );
	     }else {
	    	 System.out.println( " Недостаточно средств  " );
	     }
		}

    public static void task35() {
 // 35. Вычислить число и месяц в невисокосном году по номеру дня.   	
    	
    	int daymohth;
		int numday;

		daymohth = 0;
		numday = 399;

		if (numday > 0 && numday <= 31) {
			
			daymohth = numday;
		
			System.out.println(" Январь " + daymohth + " число");
		
		} else if (numday > 31 && numday <= 59) {
			
			daymohth = numday - 31;
			
			System.out.println(" Февраль " + daymohth + " число");
		
		} else if (numday > 59 && numday <= 90) {
			
			daymohth = numday - 59;
			
			System.out.println(" Март " + daymohth + " число");
		
		} else if (numday > 90 && numday <= 120) {
			
			daymohth = numday - 90;
			
			System.out.println(" Апрель " + daymohth + " число");
		
		} else if (numday > 120 && numday <= 151) {
			
			daymohth = numday - 120;
			
			System.out.println(" Май " + daymohth + " число");
		
		} else if (numday > 151 && numday <= 181) {
			
			daymohth = numday - 151;
			
			System.out.println(" Июнь " + daymohth + " число");
		
		} else if (numday > 181 && numday <= 212) {
			
			daymohth = numday - 181;
			
			System.out.println(" Июль " + daymohth + " число");
		
		} else if (numday > 212 && numday <= 243) {
			
			daymohth = numday - 212;
			
			System.out.println(" Август " + daymohth + " число");
		
		} else if (numday > 243 && numday <= 273) {
			
			daymohth = numday - 243;
			
			System.out.println(" Сентябрь " + daymohth + " число");
		
		} else if (numday > 273 && numday <= 304) {
			
			daymohth = numday - 273;
			
			System.out.println(" Октябрь " + daymohth + " число");
		
		} else if (numday > 304 && numday <= 334) {
			
			daymohth = numday - 304;
			
			System.out.println(" Ноябрь " + daymohth + " число");
		
		} else if (numday > 334 && numday <= 365) {
			
			daymohth = numday - 334;
			
			System.out.println("Декабрь " + daymohth + " число");
		
		} else {
			
			System.out.println("Такого дня в году  не существует ");
		}
 }
    
        public static void task36() {
 // 36. Вычислить значение функции:        	
        	double x;
    		
        	double y;

    		x = 4;

    		if (x <= 3) {
    		
    			y = x * x - 3 * x - 9;
    		
    			System.out.println("Значение функции " + y);
    	
    		} else {
    			
    			y = 1 / (Math.pow(x, 3) + 6);
    			
    			System.out.println("Значение функции" + y);
    		}
    	}
 
