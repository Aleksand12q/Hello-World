    public static void task40() {
    	
      //40. Вычислить значение функции: 
    	
    	double x;
	
    	double y;

		x = 3.5;

		if (x <= 13) {
		
			y = -(Math.pow(x, 3)) + 9;
			
			System.out.println("Значение функции" + y);
		
		} else {
			
			y = -3 / (x + 1);
			
			System.out.println("Значение функции" + y);
		}
	}
	
    	
