public class branchtask {

	public static void task1(){
	// 1. Составить программу сравнения двух чисел 1 и 2.
		//Если 1 меньше 2 – вывести на экран цифру 7, в противном случае – цифру 8. 	
	
       int a ;
		
		int b;
		
		int c ;
		
		int d;
		
		a = 1 ;
	
		b = 2;
		
		c = 7 ; //// вывод на экран если 1 больше двух
		
		d = 8;
		
		
		if ( a < b   )
		
		{  System.out.println( c );
 	}
		else {
			System.out.println(d);
		}

	
	}
	
	public static void task2() {
	//2. Составить программу сравнения двух чисел 1 и 2.
		//Если 1 меньше 2 – вывести на экран слово «yes», в противном случае – слово «no» 	
         
		int a;
		
		int b;

		a = 1;
		
		b = 2;
		
		if( a < b  ) {
		
			System.out.println( " Yes " );

	} else {
		
		System.out.println( " no " );
	}

}
		
	

 public static void task3() {
// 3. Составить программу сравнения введенного числа а и цифры 3. Вывести на экран слово «yes», если число а меньше 3; 
	 //если больше, то вывести слово «no».	 
        
		
		Scanner num   = new Scanner (System.in);
	
	   int a;
	   
	  System.out.println( " Enter a  " );
	   
	  a = num.nextInt();
	  
	if (  a < 3 ) {
		
		System.out.println( " Yes " );
	} 
	
	else {
	  
	 System.out.println(  " No   "  );
  }
}

 public static void task4() {
// 4. Составить программу:  равны ли два числа а и b? 	 

	Scanner num = new  Scanner(System.in);
		
		
		int a ;
		
		System.out.println( "Enter a " );
		
		a = num.nextInt();
		
		int b;
		
		System.out.println(  " Enter b   "  );
		
		b = num.nextInt();
	
	    if ( a == b   ) {
			
			System.out.println( " Числа a and b равны "  );
		}
		else {
			
			System.out.println(  "   Числа a and b не равны    "  );
		}

	}
	


