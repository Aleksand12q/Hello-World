     		if (x >= 3) {
     		
     			y = -(x * x) + 3 * x + 9;
     		
     			System.out.println(" Значение функции " + y);
     		
     		} else {
     		
     			y = 1 / (Math.pow(x, 3) - 6);
     			
     			System.out.println(" значение функции " + y);
     		}
     	}
   
