       int a ;

       int b ;

       int c ;

        a = 9;
 
        b = 9;

        c = 4;

if ( (a == b) && ( b == c) && ( c == a ) ) {
	
	System.out.println(   "  Треугольник abc равностороний " );

} else {
	
	System.out.println(  " Треугольник   abc не равностороний   " );
}
}
	
