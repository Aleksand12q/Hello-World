      public static void task12() {
// 12. Даны три действительных числа. Возвести в квадрат те из них, значения которых неотрицательны, и в четвертую степень — отрицательные.     	  
    
    	  int a ;
  		
  		int b ;
  		
  		int c ;
  		
  		int l; // числа неотрицательные 
  		
  		int k; // числа отрицательные 
  		
  		int p;
  		
  		a = 4;
  		
  		b = -2;
  		
  		c = -4;
  		
  		if ( a > 0   ) {
  			
  			 l = a * a ; 
  			 System.out.println( "Квадрат не отрицательногое числа a  "  + l  );
  		
  		} else {
  			  l = a * a * a *a;
  			System.out.println (  "  Четвертая степень отрицательного числа a " + l );
  		}
  		 if ( b > 0) { k = b * b;
  		 
  		 System.out.println(  " квадрат не отрицательного числа b  " + k );
  			 
  			 
  		 } else {
  			
  			 k = b * b * b * b;
  			 
  			 System.out.println(  "   Четвертая степень отрицательного числа b " + k );
  			 
  		 }
  		 
  		 if (  c > 0 ) {
  			 
  			 p = c * c ;
  			 
  			 System.out.println( "   квадрат не отрицательного числа c  " + p  );
  		 } if ( c < 0) {
  			 
  			 p = c * c * c *c;
  			 System.out.println(  "    Четвертая степень отрицательного числа c   " + p  );
  			 
  	 }
 }
      
 public static void task13() {
//13. Даны две точки А(х1, у1) и В(х2, у2). Составить алгоритм, определяющий,
//которая из точек находится ближе к началу координат. 
	 

		double x1;
		
		double x2;
		
		double y1;
		
		double y2;
		
		double A;
		
		double B;
		
		x1 = 3;
		
		x2 = 3;
		
		y1 = 3;
     
		y2 = 3;
		
		A = Math.sqrt((x1 * x1) + (y1 * y1 ));
		
		B = Math.sqrt((x2*x2) + ( y2 * y2 ));
		
		
		if ( A > B ) {
			System.out.println( " Точка B ближе к началу координат " );
			
			
		} 
		if  ( A < B   ) {
			System.out.println( " Точка А ближе к началу координат "  );
			
		}else {
			
			System.out.println( " Точки А и В находятся на одно расстоянии от начало координат "  );
			
		}
}

     public static void task14() {
 // 14. Даны два угла треугольника (в градусах). Определить, существует ли такой треугольник, 
  //и если да, то будет ли он прямоугольным
    	 
    		int a ; 
    		
    		int b;
    		
    		a = 60;
    		
    		b = 230;
    		
    		if ( (a + b) ==  90) {
    			
    			System.out.println(   " Треугольник прямоугольный    "  );
    			
    			
    		}
    		if( (a  + b)== 180  ) {
    			
    			System.out.println( " Треугольник существует   "  );
    		}
    		if ( (a + b ) < 180   ) {
    			
    			System.out.println( " Треугольника не существует   "  );
    		}
          }

    public static void task15() {
 // 15. Даны действительные числа х и у, не равные друг другу. Меньшее из этих двух чисел заменить 
 //половиной их суммы, а большее — их удвоенным произведением.    	
	int x ;
		
		int y ;
		
		int z;
		
		int e;
		
		x = 4;
		
		y = 1;
		
		z = (x + y) / 2;
	    
		e = x * y * 2 ;
		
		if (( x != y)  ) {
			if ( x > y  ) {
			
		System.out.println( z  );
			
	}
		}else {
		
		System.out.println( " x == y  "  );
	}
		if ( x < y  ) {
			
			System.out.println( e );
		}
}
 
  public static void task16() {
// 16. На плоскости ХОY задана своими координатами точка А.
//Указать, где она расположена (на какой оси или в каком координатном угле). 	  


		int x;
		
		int y;
		
		x = -6;
		
		y = -3;
		
	
		if ((x == 0) && (y == 0)) 
		{
			System.out.println(" Точка А в центре плоскости");
		} else if (x == 0)
		{
			System.out.println(" Точка А на оси Y"  );
		} 
		else if (y == 0) {
			
			System.out.println(" Точка А на оси X");
		} 
		else if (x > 0 && y > 0) {
			
			System.out.println ( "Точка А в первой четверти ");
		} 
		else if (x < 0 && y > 0) {
			
			System.out.println( "Точка А в второй четверти ");
		}
		else if (x < 0 && y < 0) {
			
			System.out.println( "Точка А в третьей четверти ");

		}
		else {
			System.out.println(" Точка А в четвертой четверти ");
		}
}  

  public static void task17() {
// 17. Даны целые числа т, п. Если числа не равны, то заменить каждое из них одним и тем же числом, 
//равным большему из исходных, а если равны, то заменить числа нулями. 	  
	  
	int m;
		
		int n;
		
		m = 4;
		
		n  = 4;
		
		if ( m != n  ) {
			
			if ( m > n  ) {
				
				n = m;}
				
				if ( n > m  ) {
					
					m = n;
	 	System.out.println( m + "  числа не равны  m > n  " );
					
	 	System.out.println( n + " числа не равны n > m  " );
				}
			}
			
	     if ( m == n  ) {
		
		n = m = 0;

		System.out.println( n + " Числа равны "   );
		
	}
 }

  public static void task18() {
//18. Подсчитать количество отрицательных среди чисел а, b, с. */	  
	
	    int a;
		
	    int b;
		
	    int c;
		
	    int count;

		a = 8;
		
		b = -2;
	
		c = -5;
		
		count = 0;

		if (a < 0) {
			
			count++;
		}
		if (b < 0) {
			
			count++;
		}
		if (c < 0) {
			
			count++;
		} else {
			
			count = 0;
		}

		System.out.println(" отрицательные числа " + count);
	}

