package by.htp.les04.main;
//36 Вычислить значение функции
public class Task_65 {

	public static void main(String[] args) {
	

		double x;
		double y;

		x = 4;

		if (x <= 3) {
			y = x * x - 3 * x - 9;
			System.out.println("Значение функции " + y);
		} else {
			y = 1 / (Math.pow(x, 3) + 6);
			System.out.println("Значение функции" + y);
		}
	}
	}
